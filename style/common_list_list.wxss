page, view, image, text, navigator {
  padding: 0;
  margin: 0;
}
.container{
  margin: 30rpx;
}
.banner {
  width: 690rpx;
  height: 300rpx;
  margin-top: -250rpx;
  border-radius: 14rpx;
  overflow: hidden;
}
.banner .img {
  width: 690rpx;
  height: 300rpx;
}
.cate-item {
    display: flex;
    flex-direction: column;
    margin: 10rpx 0;
  }
  .item {
    width: 690rpx;
    height: 488rpx;
    margin: 28rpx 0 0 0;
    border-radius: 14rpx;
    overflow: hidden;
    background-color: #fff;
    flex-wrap: wrap;
  }
  .cate-item .item .img{
    width: 690rpx;
    height: 300rpx;
  }
  .cate-footer {
    width: 630rpx;
    height: auto;
    display: flex;
    flex-direction: row;
    margin: 30rpx 30rpx 27rpx 30rpx;
  }
.cate-item .item .title {
    width: 400rpx;
    height: 68rpx;
    line-height: 35rpx;
    font-size: 28rpx;
    color: #333;
    font-weight: 400;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }

  .cate-item .item .price {
    display: block;
    height: 68rpx;
    line-height: 68rpx;
    width: 250rpx;
    text-align: center;
    color: #F55335; 
    font-weight: bold;
    font-size: 30rpx;
  }
  .rmb{
    font-size: 20rpx;
  }
  .cate-item .item .name-one {
    width: 630rpx;
    height: 30rpx;
    margin: 0 30rpx;
    overflow: hidden;
    flex-wrap: nowrap;
    font-size: 24rpx;
    color: #999; 
    font-weight: 400;
  }

/* 加载完的标识 */
.line {
  width: 90%; /* 分隔线的长度 */
  margin: auto;
  height: 1px; /* 分隔线粗细 */
  text-align: center;
  font-size: 24rpx;
  color: #ccc;
  background: #ccc;
  margin-top: 60rpx;
  margin-bottom: 22px;
}
.line span {
  position: relative;
  top: -8px;
  background: #f4f4f4;
  padding: 0px 10px;
}